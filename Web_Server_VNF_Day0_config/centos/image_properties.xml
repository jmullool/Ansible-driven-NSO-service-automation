<image_properties>
    <vnf_type>OTHER</vnf_type>
    <name>CentOS7</name>
    <version>centos7_t2_cloud</version>
    <bootup_time>600</bootup_time>
    <root_file_disk_bus>virtio</root_file_disk_bus>
    <root_image_disk_format>qcow2</root_image_disk_format>
    <vcpu_min>1</vcpu_min>
    <vcpu_max>2</vcpu_max>
    <memory_mb_min>1024</memory_mb_min>
    <memory_mb_max>4096</memory_mb_max>
    <vnic_max>8</vnic_max>
    <nocloud>true</nocloud>
    <root_disk_gb_min>20</root_disk_gb_min>
    <root_disk_gb_max>20</root_disk_gb_max>
    <console_type_serial>true</console_type_serial>
    <sriov_supported>false</sriov_supported>
    <pcie_supported>false</pcie_supported>
    <monitoring_supported>true</monitoring_supported>
    <monitoring_methods>ICMPPing</monitoring_methods>
    <low_latency>false</low_latency>
    <privileged_vm>true</privileged_vm>
    <default_profile>linux_lg</default_profile>
    <profiles><profile><name>linux_lg</name>
    <description>Linux Large with 4G RAM and 2vcpu</description>
    <vcpus>2</vcpus>
    <memory_mb>4096</memory_mb>
    <root_disk_mb>20480</root_disk_mb>
    </profile>
    <profile><name>linux_small</name>
    <description>Linux small with 1G RAM and 1vcpu</description>
    <vcpus>1</vcpus>
    <memory_mb>1024</memory_mb>
    <root_disk_mb>20480</root_disk_mb>
    </profile>
    </profiles>
    <cdrom>true</cdrom>
    <interface_hot_add>true</interface_hot_add>
    <interface_hot_delete>false</interface_hot_delete>
    <bootstrap_file_1>user-data</bootstrap_file_1>
    <bootstrap_file_2>meta-data</bootstrap_file_2>
    <custom_property>
        <IP_ADDRESS> </IP_ADDRESS>
    </custom_property>
    <custom_property>
        <NETMASK> </NETMASK>
    </custom_property>
    <custom_property>
        <GATEWAY> </GATEWAY>
    </custom_property>
</image_properties>
